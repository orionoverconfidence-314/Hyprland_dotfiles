{
    // Hyprland workspaces module
    "hyprland/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "warp-on-scroll": false,
        "format": "{name}"
    },
    // Keyboard state module
    "keyboard-state": {
        "numlock": true,
        "capslock": true,
        "format": "{name} {icon}",
        "format-icons": {
            "locked": "ÔÄ£",
            "unlocked": "ÔÇú"
        }
    },
    // Clock module
    "clock": {
        "format": "{:%H:%M} ÔÄó ",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format-alt": "{:%d-%B-%Y}"
    },
    // Backlight module
    "backlight": {
        "format": "{percent}% {icon}",
        "tooltip-format": "Brightness: {percent}% ",
        "format-icons": [
            "Óéç",
            "Óèì",
            "Óèë",
            "Óèè",
            "Óèé",
            "Óèç",
            "Óèä",
            "Óèà",
            "Óéõ"
        ]
    },
    // Battery module
    "battery": {
        "states": {
            "warning": 30,
            "critical": 15
        },
        "format": "{capacity}% {icon}",
        "format-full": "{capacity}% {icon}",
        "format-charging": "{capacity}% Ôóß",
        "format-plugged": "{capacity}% Ôá¶",
        "format-alt": "{time} {icon}",
        "format-icons": [
            "ÔâÑ",
            "ÔâÉ",
            "ÔâÇ",
            "ÔâÅ",
            "ÔâÄ"
        ]
    },
    // Idle inhibitor module
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "‚òÄÔ∏è",
            "deactivated": "üåô"
        }
    },
    // Power profiles module
    "power-profiles-daemon": {
        "format": "{icon}",
        "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
        "tooltip": true,
        "format-icons": {
            "default": "ÔÉß",
            "performance": "ÔÉß",
            "balanced": "Ôâé",
            "power-saver": "ÔÅ¨"
        }
    },
    // PulseAudio module
    "pulseaudio": {
        "format": "{volume}% {icon} {format_source}",
        "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
        "format-bluetooth-muted": "Ôö© {icon}Ôäî {format_source}",
        "format-muted": "Ôö© {format_source}",
        "format-source": " {volume}% ÔÑ∞",
        "format-source-muted": "ÔÑ±",
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "Ôñê",
            "headset": "Ôñê",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": [
                "ÔÄ¶",
                "ÔÄß",
                "ÔÄ®"
            ]
        },
        "on-click": "pavucontrol"
    },
    // CPU
    "cpu": {
        "format": "{usage}% Ôãõ",
        "tooltip": true,
        "tooltip-format": "Power profile: {profile}\nDriver: {driver}"
    },
    // Memory
    "memory": {
        "interval": 5,
        "format": "{used} / {total} GB ÔÉâ", // only percentage works on 0.14
        "tooltip-format": "Used: {used} GB\nTotal: {total} GB\nPercentage: {percentage}%",
        "states": {
            "warning": 60,
            "critical": 80
        },
        "on-click": "foot -e htop",
        "on-click-right": "notify-send 'Freeing Cache'; sync; echo 3 | sudo tee /proc/sys/vm/drop_caches"
    },
    // Disk
    "disk": {
        "interval": 10,
        "path": "/",
        "format": "{used} / {total}  ({percentage_used}%) ÔÇ†",
        "format-alt": "Free: {free} GB ÔÇ†",
        "format-alt-click": "right",
        "tooltip-format": "Disk ({path})\nUsed: {used} GB\nFree: {free} GB\nTotal: {total} GB\nUsage: {percentage_used}%",
        "states": {
            "warning": 70,
            "critical": 90
        }
    },
    // Temperature
    "temperature": {
        // "thermal-zone": 2,
        // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
        "critical-threshold": 80,
        "format-critical": "{temperatureC}¬∞C {icon}",
        "format": "{temperatureC}¬∞C {icon}",
        "format-icons": [
            "Ôù´",
            "Ôãâ",
            "Ôù©"
        ]
    }
